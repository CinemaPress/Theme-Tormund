<!DOCTYPE html>
<html lang="<%- page.language %>"
      prefix="og: http://ogp.me/ns# video: http://ogp.me/ns/video# ya: http://webmaster.yandex.ru/vocabularies/">

<head>
    <%- include('includes/head.ejs'); -%>
</head>

<body class="fluid">

<% /* Брендирование страницы (сразу после <body>) */ %>
<% if (typeof page.adv === 'object' && page.adv.brand) { %>
    <%- page.adv.brand %>
<% } %>

<div data-gets="" data-type="pgskin"></div>
<main id="wrapper">
    <header id="header">
        <%- include('includes/header.ejs'); -%>
    </header>
    <div id="wrapper-inner">
        <div class="triggered-overlay"></div>

        <%- include('includes/menu.ejs'); -%>

        <div id="content">
            <div class="inner-content container">
                <div id="router-view">

                    <% /* Реклама вверху страницы */ %>
                    <% if (typeof page.adv === 'object' && page.adv.top) { %>
                        <div style="text-align:center;margin:10px auto">
                            <%- page.adv.top %>
                        </div>
                    <% } %>

                    <div class="bg-cover-faker">
                        <div class="ui grid mb-0">
                            <div class="left floated sixteen wide tablet wide computer column pb-0">
                                <h1 class="page-title pt-sm pb-sm"><%- page.h1 %></h1>
                                <% /* Описание страницы */ %>
                                <% if (page.seo) { %>
                                    <div class="description-primary pb-0 mb-md">
                                        <%- page.seo %>
                                    </div>
                                <% } %>
                            </div>
                        </div>

                        <% /* Контент новости */ %>
                        <% if (typeof content === 'object') { %>
                            <div class="cinemapress-content">

                                <% /* Изображение новости */ %>
                                <% if (content.image) { %>
                                    <img src="<%- content.image %>" class="cinemapress-content-image" alt="Image">
                                <% } %>

                                <% /* Описание новости */ %>
                                <% if (content.description) { %>
                                    <div class="cinemapress-content-description">
                                        <%- content.description %>
                                    </div>
                                <% } %>

                                <div class="cinemapress-content-publish" style="vertical-align: middle;">

                                    <% /* Список тегов */ %>
                                    <% if (content.tags) { %>
                                        <span class="cinemapress-content-publish-tags" style="vertical-align: middle;">
                                            <img
                                                    src="data:image/svg+xml;utf8;base64,"
                                                    alt="Tags"
                                                    style="width: 10px;vertical-align: middle;"> <%- content.tags %>
                                        </span>
                                    <% } %>

                                    <% /* Дата публикации */ %>
                                    <% if (content.publish) { %>
                                        <span class="cinemapress-content-publish-publish"
                                              style="vertical-align: middle;"><img
                                                    src="data:image/svg+xml;utf8;base64,"
                                                    alt="Publish"
                                                    style="width: 10px;vertical-align: middle;"> <%- content.publish %>
                                        </span>
                                    <% } %>

                                </div>

                            </div>
                        <% } %>

                        <% /* Блок случайного фильма в категории */ %>
                        <div class="randomMovieCategory">
                            <div class="cinemapress-random-category"
                                 onclick="window.location.href='<%- page.url %><%- (page.url.indexOf('?') + 1 ? '&' : '?') %>random=<%- Math.random() %>'">
                                <img src="/themes/<%- page.theme %>/public/images/dice.svg" alt="<%- page.l.random %>">
                                <span><%- page.l.lucky %></span>
                                <small><%- page.l.random %></small>
                            </div>
                        </div>

                        <div class="ui grid mb-lg mt-0">
                            <% /* Сортировка фильмов */ %>
                            <% if (typeof page.sorting === 'object' && page.sorting.length) { %>
                                <div class="right aligned floated sixteen wide column mobile-only">
                                    <div class="filter-mobile">
                                        <div class="fm-order">
                                            <div class="ui secondary dropdown top left pointing selection">
                                                <i class="dropdown icon"></i>
                                                <% var sort_active = true; %>
                                                <% page.sorting.forEach(function (sort) { %>
                                                    <% if (sort.active) { %>
                                                        <% sort_active = false; %>
                                                        <div class="text"><%- sort.name %></div>
                                                    <% } %>
                                                <% }); %>
                                                <% if (sort_active) { %>
                                                    <div class="text"><%- page.l.sorting %></div>
                                                <% } %>
                                                <div class="menu transition hidden" tabindex="-1">
                                                    <% page.sorting.forEach(function (sort) { %>

                                                        <% /* Текущая сортировка */ %>
                                                        <% if (sort.active) { %>
                                                            <a href="<%- sort.url %>" rel="nofollow"
                                                               class="item"><%- sort.name %></a>
                                                        <% } else { %>
                                                            <a href="<%- sort.url %>" rel="nofollow"
                                                               class="item"><%- sort.name %></a>
                                                        <% } %>

                                                    <% }); %>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>

                            <% /* Сортировка фильмов */ %>
                            <% if (typeof page.sorting === 'object' && page.sorting.length) { %>
                                <div class="right aligned floated sixteen wide column desktop-only">
                                    <div class="ui secondary dropdown top left pointing selection">
                                        <i class="dropdown icon"></i>
                                        <% var sort_active2 = true; %>
                                        <% page.sorting.forEach(function (sort) { %>
                                            <% if (sort.active) { %>
                                                <% sort_active2 = false; %>
                                                <div class="text"><%- sort.name %></div>
                                            <% } %>
                                        <% }); %>
                                        <% if (sort_active2) { %>
                                            <div class="text"><%- page.l.sorting %></div>
                                        <% } %>
                                        <div class="menu transition hidden" tabindex="-1">
                                            <% page.sorting.forEach(function (sort) { %>

                                                <% /* Текущая сортировка */ %>
                                                <% if (sort.active) { %>
                                                    <a href="<%- sort.url %>" rel="nofollow"
                                                       class="item"><%- sort.name %></a>
                                                <% } else { %>
                                                    <a href="<%- sort.url %>" rel="nofollow"
                                                       class="item"><%- sort.name %></a>
                                                <% } %>

                                            <% }); %>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                            <div class="left floated sixteen wide tablet <% if (typeof content !== 'object') { %>twelve<% } else { %>sixteen<% } %> wide computer column">
                                <div id="discover-response" class="dark-segment">
                                    <% /* Цикл фильмов */ %>
                                    <% if (typeof movies === 'object' && movies.length) { %>
                                        <ul class="filter-results">
                                            <% movies.forEach(function (movie) { %>
                                                <li class="mb-lg">
                                                    <div class="poster-with-subject">
                                                        <a href="<%- movie.url %>">
                                                            <img src="<%- movie.poster_min %>" data-cinemapress-src="<%- movie.poster %>" alt="<%= movie.title %>">
                                                        </a>
                                                        <div class="subject">
                                                            <div class="subject-title">
                                                                <div>
                                                                    <a href="<%- movie.url %>">
                                                                        <h2 class="truncate"><%- movie.title %></h2>
                                                                    </a>
                                                                    <p class="poster-meta truncate">
                                                                        <span class="genres"><%- movie.genres %></span>
                                                                        <% if (movie.rating) { %>
                                                                            <span class="rating">
                                                                                <svg class="mofycon">
                                                                                    <use xlink:href="#icon-star"></use>
                                                                                </svg> <%- movie.rating / 10 %>
                                                                            </span>
                                                                        <% } %>
                                                                    </p>
                                                                    <p class="poster-meta truncate">
                                                                        <% if (movie.directors) { %>
                                                                            <strong><%- page.l.directors %></strong>: <%- movie.directors %><br>
                                                                        <% } %>
                                                                        <% if (movie.actors) { %>
                                                                            <strong><%- page.l.actors %></strong>: <%- movie.actors %>
                                                                        <% } %>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                            <p class="desktop-only excerpt">
                                                                <%- movie.description_short %><% if (movie.description_short[movie.description_short.length-1] !== '.') { %>...<% } %>
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <p class="mobile-only excerpt">
                                                        <%- movie.description_short %><% if (movie.description_short[movie.description_short.length-1] !== '.') { %>...<% } %>
                                                    </p>
                                                    <div class="media-meta">
                                                        <table class="ui single line celled unstackable table">
                                                            <tbody>
                                                            <tr>
                                                                <% if (movie.year) { %>
                                                                    <td>
                                                                        <div><%- page.l.year %></div>
                                                                        <div><%- movie.year %></div>
                                                                    </td>
                                                                <% } %>
                                                                <% if (movie.country) { %>
                                                                    <td>
                                                                        <div><%- page.l.country %></div>
                                                                        <div><%- movie.country %></div>
                                                                    </td>
                                                                <% } %>
                                                                <% if (movie.kp_vote) { %>
                                                                    <td>
                                                                        <div><%- page.l.kp %></div>
                                                                        <div><%- movie.kp_rating / 10 %></div>
                                                                    </td>
                                                                <% } %>
                                                                <% if (movie.imdb_vote) { %>
                                                                    <td>
                                                                        <div><%- page.l.imdb %></div>
                                                                        <div><%- movie.imdb_rating / 10 %></div>
                                                                    </td>
                                                                <% } %>
                                                            </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </li>
                                            <% }); %>
                                        </ul>
                                    <% } %>
                                </div>

                                <% /* Пагинация */ %>
                                <%- include('includes/pagination.ejs'); -%>

                                <% /* Блок с комментариями для новости */ %>
                                <% if (typeof content === 'object') { %>
                                    <%- include('includes/comments.ejs'); -%>
                                <% } %>
                            </div>
                            <% if (typeof content !== 'object') { %>
                                <div class="left floated sixteen wide tablet four wide computer column"
                                     id="filter-sidebar">
                                    <div class="global-box pl-0 pt-0">
                                        <div class="ui middle aligned divided list">
                                            <div class="item just-title">
                                                <a href="/<%- page.urls.type %>/<%- page.urls.types.serial %>">
                                                    <h2 class="truncate pt-0"><%- page.l.series %></h2>
                                                </a>
                                            </div>
                                            <div class="item just-title">
                                                <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>">
                                                    <h2 class="truncate pt-0"><%- page.l.movies %></h2>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="section-heading"><%- page.l.years %></div>
                                    <div class="global-box pl-0 pt-0">
                                        <div class="ui middle aligned divided list">
                                            <div class="item just-title">
                                                <% /* Годы (список в настройках отображения) */ %>
                                                <% page.urls.years.slice(0, 5).forEach(function(year) { %>
                                                    <a href="<%- year.url %>">
                                                        <h2 class="truncate pt-0"><%- year.title %></h2>
                                                    </a>
                                                <% }); %>
                                            </div>
                                            <div class="item just-title">
                                                <% /* Годы (список в настройках отображения) */ %>
                                                <% page.urls.years.slice(5, 10).forEach(function(year) { %>
                                                    <a href="<%- year.url %>">
                                                        <h2 class="truncate pt-0"><%- year.title %></h2>
                                                    </a>
                                                <% }); %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="section-heading"><%- page.l.countries %></div>
                                    <div class="global-box pl-0 pt-0">
                                        <div class="ui middle aligned divided list">
                                            <div class="item just-title">
                                                <% /* Годы (список в настройках отображения) */ %>
                                                <% page.urls.countries.slice(0, 5).forEach(function(country) { %>
                                                    <a href="<%- country.url %>">
                                                        <h2 class="truncate pt-0"><%- country.title %></h2>
                                                    </a>
                                                <% }); %>
                                            </div>
                                            <div class="item just-title">
                                                <% /* Годы (список в настройках отображения) */ %>
                                                <% page.urls.countries.slice(5, 10).forEach(function(country) { %>
                                                    <a href="<%- country.url %>">
                                                        <h2 class="truncate pt-0"><%- country.title %></h2>
                                                    </a>
                                                <% }); %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer class="footer">
        <%- include('includes/footer.ejs'); -%>
    </footer>
</main>
</body>

</html>